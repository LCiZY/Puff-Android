<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <layer-list>
            <item android:drawable="@drawable/shape_key_special_p" />
            <item
                android:bottom="@dimen/keyboard_key_top"
                android:drawable="@drawable/sym_keyboard_return2"
                android:left="@dimen/keyboard_key_left"
                android:right="@dimen/keyboard_key_left"
                android:top="@dimen/keyboard_key_top" />
        </layer-list>
    </item>
    <item>
        <layer-list>
            <!--用下面这个背景，把系统的背景覆盖掉-->
            <item android:drawable="@drawable/shape_key_special_n" />
            <item
                android:bottom="@dimen/keyboard_key_top"
                android:drawable="@drawable/sym_keyboard_return2"
                android:left="@dimen/keyboard_key_left"
                android:right="@dimen/keyboard_key_left"
                android:top="@dimen/keyboard_key_top" />
        </layer-list>
    </item>
</selector>